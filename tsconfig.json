{
    "compilerOptions": {
        "target": "ES2022",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "esModuleInterop": true,
        "strict": false,
        "skipLibCheck": true,
        "allowJs": true,
        "rootDir": ".",
        "outDir": "dist"
    },
    "include": [
        "api/**/*"
    ],
    "exclude": [
        "node_modules",
        "csp-backend",
        "csp-frontend"
    ]
}